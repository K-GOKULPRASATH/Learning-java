// User function Template for Java

class Solution {
    int[] getMoreAndLess(int[] arr, int target) {
        // code here
        int[] ret = new int[2];
        ret[0] = 0;
        ret[1] = 0;
        for(int i = 0; i < arr.length; i++){
            if(arr[i] <= target){
            ret[0]++;   
            }
            if(arr[i] >= target){
            ret[1]++;
            }
            
        }
        return ret;
    }
}
