
class Solution {
    
    public static boolean isPalindrom(int num){
        int original = num;
        int reversed = 0;
        while (num >0){
            int digit = num%10;
            reversed = reversed*10 + digit;
            num = num /10;
        }
        if(original == reversed){
            return true;
        }else
        return false;
    } 
    
    
    public static boolean isPalinArray(int[] arr) {
        for (int num : arr){
            if(!isPalindrom(num)){
                return false;
            }
        }
        
        return true;

    }
}
