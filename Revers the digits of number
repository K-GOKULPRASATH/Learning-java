import java.util.Scanner;

class Main{
    public static void main(String[] args) {
        Scanner i = new Scanner(System.in);
        int n = i.nextInt();
        int temp = n;
        int rev =0;
        while (temp!=0){
            rev = rev*10 +temp%10;
            temp = temp/10;
        }
        System.out.println(rev);
    }
}
