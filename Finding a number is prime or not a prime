import java.util.Scanner;
class Main{
    public static void main(String[]args){
        Scanner scan = new Scanner(System.in);
        int num = scan.nextInt();
        int temp = num;
        int count = 0;
        for(int i = 2; i<=num/2; i++){
            if(num%i ==0)
            count++;
            break;
        }
        if(count !=0)
        System.out.println("no");
        else 
            System.out.println("yes"+temp);
    }
}
