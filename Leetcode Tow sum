class Solution {
    public int[] twoSum(int[] nums, int target) {
        Map<Integer, Integer> map = new HashMap<>();
        for(int i = 0; i < nums.length; i++) {
            int cur = nums[i];
            int rem = target - cur;
            if(map.containsKey(rem)) return new int[] {i, map.get(rem)};
            map.put(cur, i);
        }
        return new int[] {};
    }
}
