import java.util.Scanner;
class Solution {
    public boolean isPalindrome(int x) {
        int res = 0;
        int temp = x;
        while (temp > 0 ){
            res = res*10 + temp%10;
            temp = temp/10;
            
        }
        if(x == res)
           return true;
        else
            return false;
        
    }
}
